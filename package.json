    "cross-env": "^7.0.3",
    "jasmine-core": "5.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "postcss": "^8.4.49",
    "protractor": "^7.0.0",
    "ts-node": "10.9.2",
    "tslint": "6.1.3",
    "typescript": "5.6"
  }
